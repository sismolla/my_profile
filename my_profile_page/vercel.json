{
    "version": 2,
    "builds": [
      {
        "src": "profile_page/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python"
        }
      },
      
      {
        "src": "build.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "static"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/staticfiles/$1"
      },

      
      {
        "src": "/(.*)",
        "dest": "profile_page/wsgi.py"
      }
      
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "profile_page.settings",
    "PYTHON_VERSION": "3.11.2"
  }
}
  